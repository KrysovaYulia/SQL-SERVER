# Вывести список студентов, сдавших минимум два экзамена. 
select С.Фамилия 
from Студент С 
left outer join Экзамен Э ON С.Рег_номер = Э.Рег_номер
where 
Э.Рег_номер >= 2
order by С.Фамилия ;

# Вывести список преподавателей с зарплатой, меньшей 20000 руб
select С.Фамилия, С.Зарплата
from Сотрудник С 
left outer join Преподаватель П ON С.Таб_номер = П.Таб_номер
where С.Зарплата < 20000;
